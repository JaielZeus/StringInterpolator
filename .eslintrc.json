{
    "env": {
        "node": true,
        "es2022": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2022
    },
    "rules": {
        "linebreak-style": "off",
        "no-param-reassign": "off",
        "no-console": "off",
        "max-len": "off"
    }
}